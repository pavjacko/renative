{
    "env": {
        "SOME_ENV_VAR": "xxxx"
    },
    "projectName": "renative",
    "enableHookRebuild": false,
    "workspaceID": "rnv",
    "isWrapper": true,
    "isMonorepo": true,
    "paths": {
        "appConfigsDir": "./appConfigs",
        "platformAssetsDir": "./platformAssets",
        "platformBuildsDir": "./platformBuilds"
    },
    "plugins": {
        "@noriginmedia/react-spatial-navigation": "source:rnv",
        "next": "source:rnv"
    },
    "engines": {
        "@rnv/engine-rn": "source:rnv",
        "@rnv/engine-rn-tvos": "source:rnv",
        "@rnv/engine-rn-web": "source:rnv",
        "@rnv/engine-rn-next": "source:rnv",
        "@rnv/engine-rn-electron": "source:rnv",
        "@rnv/engine-lightning": "source:rnv"
    },
    "integrations": {
        "@rnv/integration-vercel": "source:rnv",
        "@rnv/integration-docker": "source:rnv"
    },
    "tasks": {
        "install": {
            "script": "yarn bootstrap"
        }
    },
    "defaults": {
        "ports": {
            "ios": 8182,
            "android": 8183,
            "androidtv": 8184,
            "firetv": 8198,
            "tvos": 8185,
            "macos": 8186,
            "web": 8180,
            "tizen": 8187,
            "webos": 8188,
            "androidwear": 8189,
            "tizenwatch": 8190,
            "tizenmobile": 8191,
            "windows": 8192,
            "kaios": 8193,
            "firefoxos": 8194,
            "firefoxtv": 8114
        },
        "supportedPlatforms": [
            "ios",
            "android",
            "androidtv",
            "firetv",
            "androidwear",
            "web",
            "webtv",
            "tizen",
            "tvos",
            "webos",
            "macos",
            "windows",
            "tizenwatch",
            "tizenmobile",
            "kaios",
            "firefoxos",
            "firefoxtv",
            "chromecast"
        ],
        "schemes": {
            "run": "debug",
            "build": "release",
            "export": "release"
        },
        "targets": {}
    },
    "templates": {
        "renative-template-hello-world": {
            "version": "~0.32.0-RC",
            "webpack": {
                "moduleAliases": true
            }
        },
        "renative-template-blank": {
            "version": "~0.32.0-RC",
            "webpack": {
                "moduleAliases": true
            }
        },
        "renative-template-kitchen-sink": {
            "version": "~0.32.0-RC",
            "webpack": {
                "moduleAliases": true
            }
        }
    },
    "platforms": {
        "web": {
            "engine": "engine-rn-next"
        },
        "tvos": {
            "engine": "engine-rn-tvos"
        },
        "android": {
            "mainActivity": {
                "onCreate": "super.onCreate(null)"
            }
        }
    },
    "currentTemplate": "renative-template-hello-world",
    "crypto": {
        "optional": true,
        "encrypt": {
            "dest": "PROJECT_HOME/ci/privateConfigs.enc"
        },
        "decrypt": {
            "source": "PROJECT_HOME/ci/privateConfigs.enc"
        }
    }
}